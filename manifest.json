{
  "name": "TidyTab",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "将已经打开的标签页集中保存管理，有效减少标签混乱以及释放资源。",
  "icons": {
    "16": "icons/nasapod16x16.png",
    "32": "icons/nasapod32x32.png",
    "48": "icons/nasapod48x48.png",
    "128": "icons/nasapod128x128.png"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": ["images/*.jpeg"],
  "options_ui": {
    "open_in_tab": true,
    "page": "/main/index.html"
  },
  "background": {
    "page": "main/bg.html"
  }
}
